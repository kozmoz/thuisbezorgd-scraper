{
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jasmine": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "semi": [
      1,
      "always"
    ],
    "comma-dangle": [
      1,
      "never"
    ],
    "quotes": [
      2,
      "single"
    ]
  }
}
